{"ast":null,"code":"import { getTableData } from \"@/utils/table\";\nexport default {\n  name: \"WorkList\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      page: 1,\n      size: 10,\n      loading: true // 页面一直转圈在加载\n    };\n  },\n\n  methods: {\n    handleSizeChange(val) {\n      this.size = val;\n      this.page = 1;\n      // 改完之后需重新请求\n      getTableData(this, \"/works\", {\n        page: this.page,\n        size: this.size\n      }, this.tableData);\n    },\n    handleCurrentChange(val) {\n      this.page = val;\n      getTableData(this, \"/works\", {\n        page: this.page,\n        size: this.size\n      }, this.tableData);\n    }\n  },\n  // 后段分页，传入page，size\n  created() {\n    getTableData(this, \"/works\", {\n      page: this.page,\n      size: this.size\n    }, this.tableData);\n  }\n};","map":{"version":3,"names":["getTableData","name","data","tableData","total","page","size","loading","methods","handleSizeChange","val","handleCurrentChange","created"],"sources":["/Users/mac/Desktop/vue_bsm/vue3_bsm/src/components/Student/WorkList.vue"],"sourcesContent":["<template>\n  <div class=\"workList\">\n    <el-table :data=\"tableData\" v-loading=\"loading\" border style=\"width: 100%\">\n      <!-- width+180px 改成 align=\"center\" 居中显示去除滚动条 -->\n      <el-table-column prop=\"id\" label=\"用户id\" align=\"center\">\n      </el-table-column>\n      <el-table-column prop=\"userId\" label=\"班级\" align=\"center\">\n      </el-table-column>\n      <el-table-column prop=\"title\" label=\"作业名称\" align=\"center\">\n      </el-table-column>\n      <el-table-column prop=\"completed_text\" label=\"完成情况\" align=\"center\">\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :current-page=\"page\"\n      :page-sizes=\"[10, 20, 30, 40, 50]\"\n      :page-size=\"size\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n    >\n    </el-pagination>\n  </div>\n</template>\n\n<script>\nimport { getTableData } from \"@/utils/table\";\nexport default {\n  name: \"WorkList\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      page: 1,\n      size: 10,\n      loading: true, // 页面一直转圈在加载\n    };\n  },\n  methods: {\n    handleSizeChange(val) {\n      this.size = val;\n      this.page = 1;\n      // 改完之后需重新请求\n      getTableData(\n        this,\n        \"/works\",\n        { page: this.page, size: this.size },\n        this.tableData\n      );\n    },\n    handleCurrentChange(val) {\n      this.page = val;\n      getTableData(\n        this,\n        \"/works\",\n        { page: this.page, size: this.size },\n        this.tableData\n      );\n    },\n  },\n  // 后段分页，传入page，size\n  created() {\n    getTableData(\n      this,\n      \"/works\",\n      { page: this.page, size: this.size },\n      this.tableData\n    );\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.workList {\n  .el-pagination {\n    text-align: left;\n    margin-top: 20px;\n  }\n}\n</style>\n"],"mappings":"AA2BA,SAASA,YAAW,QAAS,eAAe;AAC5C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,EAAE;MACbC,KAAK,EAAE,CAAC;MACRC,IAAI,EAAE,CAAC;MACPC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,IAAI,CAAE;IACjB,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,GAAG,EAAE;MACpB,IAAI,CAACJ,IAAG,GAAII,GAAG;MACf,IAAI,CAACL,IAAG,GAAI,CAAC;MACb;MACAL,YAAY,CACV,IAAI,EACJ,QAAQ,EACR;QAAEK,IAAI,EAAE,IAAI,CAACA,IAAI;QAAEC,IAAI,EAAE,IAAI,CAACA;MAAK,CAAC,EACpC,IAAI,CAACH,SAAQ,CACd;IACH,CAAC;IACDQ,mBAAmBA,CAACD,GAAG,EAAE;MACvB,IAAI,CAACL,IAAG,GAAIK,GAAG;MACfV,YAAY,CACV,IAAI,EACJ,QAAQ,EACR;QAAEK,IAAI,EAAE,IAAI,CAACA,IAAI;QAAEC,IAAI,EAAE,IAAI,CAACA;MAAK,CAAC,EACpC,IAAI,CAACH,SAAQ,CACd;IACH;EACF,CAAC;EACD;EACAS,OAAOA,CAAA,EAAG;IACRZ,YAAY,CACV,IAAI,EACJ,QAAQ,EACR;MAAEK,IAAI,EAAE,IAAI,CAACA,IAAI;MAAEC,IAAI,EAAE,IAAI,CAACA;IAAK,CAAC,EACpC,IAAI,CAACH,SAAQ,CACd;EACH;AACF,CAAC"},"metadata":{},"sourceType":"module","ex